			if (!$(this).parent('li').hasClass("over")) {
				$('.faq_list .faq_a').hide(0);
				$(".faq_list li").removeClass("over");
				$(this).parent('li').find("div.faq_a").stop(false,true).show(0,function(){
					$(this).parent('li').addClass("over");
				});
			} else {
				$(".faq_list .faq_a").hide(0,function(){
					$(".faq_list li").removeClass("over");
				});
			}
		});
	});

